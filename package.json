{
  "name": "@ilovehanekawa/sprint",
  "version": "1.1.6",
  "type": "module",
  "description": "SMTP mailing using Nodemailer and Gmail",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"vite\" \"cd router && tsc.cmd -w\" \"cd test-app && tsc.cmd -w\" \"cd test-app && npx nodemon index.js\"",
    "actions": "concurrently \"vite\" \"cd test-app && npx nodemon index.js\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "concurrently \"vite preview\" \"cd router && tsc.cmd -w\" \"cd test-app && tsc.cmd -w\" \"cd test-app && npx nodemon index.js\""
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iLoveHanekawa/sprint.git"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
